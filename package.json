{
    "name": "cars",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "knex:migrate": "knex migrate:latest --knexfile knexfile.ts",
        "knex:migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
        "knex:seed": "knex seed:run --knexfile knexfile.ts",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
        "lint": "eslint src --max-warnings=0",
        "typecheck": "tsc --project tsconfig.json --noEmit"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{json,css,scss,md,svg}": [
            "prettier --write"
        ],
        "**/*.css": "stylelint --fix",
        "**/*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "dependencies": {
        "@next/bundle-analyzer": "^9.5.2",
        "@storybook/react": "^6.5.16",
        "@tailwindcss/ui": "^0.6.2",
        "axios": "^0.19.2",
        "classnames": "^2.3.2",
        "cross-env": "^7.0.2",
        "dayjs": "^1.11.7",
        "formik": "^2.1.5",
        "knex": "^0.21.2",
        "next": "^13.0.0",
        "next-compose-plugins": "^2.2.0",
        "nprogress": "^0.2.0",
        "react": "18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^3.10.0",
        "react-particles": "^2.9.3",
        "sqlite3": "^5.0.0",
        "swr": "^0.3.0",
        "tailwindcss": "^3.2.6",
        "tsparticles": "^2.9.3",
        "tsparticles-engine": "^2.9.3"
    },
    "devDependencies": {
        "@types/node": "^14.0.27",
        "@types/nprogress": "^0.2.0",
        "@types/react": "^16.9.44",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "eslint": "^7.6.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-react-hooks": "^4.0.8",
        "eslint-plugin-standard": "^4.0.1",
        "eslint-plugin-tailwind": "^0.2.0",
        "husky": "^4.3.0",
        "lint-staged": "^10.5.1",
        "postcss-preset-env": "^8.0.1",
        "prettier": "^2.0.5",
        "ts-node": "^8.10.2",
        "ts-node-dev": "^1.0.0-pre.56",
        "typescript": "^4.9.5"
    }
}

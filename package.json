{
    "name": "cars",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "knex:migrate": "knex migrate:latest --knexfile knexfile.ts",
        "knex:migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
        "knex:seed": "knex seed:run --knexfile knexfile.ts",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "generate-icons": "node ./scripts/generate-icon-components.js",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
        "lint": "eslint src --max-warnings=0",
        "typecheck": "tsc --project tsconfig.json --noEmit"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix"
        ]
    },
    "dependencies": {
        "@next/bundle-analyzer": "^9.5.2",
        "@storybook/react": "^6.5.16",
        "@tailwindcss/ui": "^0.6.2",
        "axios": "^0.19.2",
        "classnames": "^2.3.2",
        "cross-env": "^7.0.2",
        "depcheck": "^1.4.3",
        "eslint-config-next": "^13.1.6",
        "formik": "^2.1.5",
        "knex": "^2.3.0",
        "lint": "^0.7.0",
        "next": "^13.1.6",
        "next-compose-plugins": "^2.2.0",
        "nprogress": "^0.2.0",
        "postcss-preset-env": "^8.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^3.10.0",
        "react-particles": "^2.9.3",
        "sqlite3": "^5.1.2",
        "swr": "^0.3.0",
        "tailwindcss": "^3.2.6",
        "tsparticles": "^2.9.3",
        "tsparticles-engine": "^2.9.3"
    },
    "devDependencies": {
        "@types/node": "^14.0.27",
        "@types/nprogress": "^0.2.0",
        "@types/react": "^16.9.44",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.5",
        "husky": "^4.3.0",
        "lint-staged": "^10.5.1",
        "prettier": "^2.0.5",
        "typescript": "^4.8.4"
    }
}
